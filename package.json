{
  "name": "express-restapi-ts-docker",
  "version": "1.0.0",
  "description": "starter template for RESTful API with TypeScript and Docker",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc --project ./tsconfig.json",
    "start": "node ./build/server.js",
    "serve": "tsc-watch --project tsconfig.json --onSuccess 'npm run start'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.3.5"
  }
}
